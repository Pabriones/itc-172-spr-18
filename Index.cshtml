@model  IEnumerable<FirstLook.Models.Mailing>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>@Html.DisplayFor(model => Model.FirstName)</th>
        <th>@Html.DisplayFor(model => Model.LastName)</th>
        <th>@Html.DisplayFor(model => Model.Email)</th>
    </tr>
    @foreach(var item in Model)
    {
    <tr>
        <td>@Html.DisplayFor(model => item.FirstName)</td>
        <td>@Html.DisplayFor(model => item.LastName)</td>
        <td>@Html.DisplayFor(model => item.Email)</td>
    </tr>
    }
</table>
